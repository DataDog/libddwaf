{
	"manifest": {
		"param": {
			"inherit_from": "param",
			"run_on_key": true,
			"run_on_value": true
		}
	},
	"rules": [
		{
			"operator": "@invalid",
			"targets": ["param"],
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": ".",
			"targets": ["param"],
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": 42,
			"targets": ["param"],
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": null,
			"targets": ["param"],
			"transformations": [],
			"value": "bla"
		},
		{
			"targets": ["param"],
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": "@rx",
			"targets": [],
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": "@rx",
			"targets": 42,
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": "@rx",
			"targets": null,
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": "@rx",
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": "@rx",
			"targets": [
				"param",
				42
			],
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": "@rx",
			"targets": [
				"param",
				null
			],
			"transformations": [],
			"value": "bla"
		},
		{
			"operator": "@rx",
			"targets": ["param"],
			"transformations": [],
			"value": 42
		},
		{
			"operator": "@rx",
			"targets": ["param"],
			"transformations": [],
			"value": null
		},
		{
			"operator": "@rx",
			"targets": ["param"],
			"transformations": []
		},
		{
			"operator": "@rx",
			"targets": ["param"],
			"transformations": [42],
			"value": "test"
		},
		{
			"operator": "@rx",
			"targets": ["param"],
			"transformations": [null],
			"value": "test"
		},
		{
			"operator": "@rx",
			"targets": ["param"],
			"transformations": ["fake transformation"],
			"value": "test"
		},
		{
			"operator": "@rx",
			"targets": ["param"],
			"transformations": ["lowercase", 42],
			"value": "test"
		},
		{
			"operator": "@eq",
			"targets": ["param"],
			"transformations": [],
			"value": 42.5
		},
		{
			"operator": "@lt",
			"targets": ["param"],
			"transformations": [],
			"value": [42]
		}
	]
	

}
